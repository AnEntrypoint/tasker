{
  "name": "gmail-search-tasker",
  "description": "Comprehensive Gmail search with QuickJS asyncify support",
  "type": "module",
  "main": "comprehensive-gmail-search-cli.js",
  "scripts": {
    "start": "supabase start",
    "serve": "supabase functions serve --no-verify-jwt tasks stack-processor quickjs wrappedgapi wrappedkeystore",
    "test:gmail": "concurrently --kill-others \"npm run serve\" \"node comprehensive-gmail-search-cli.js\"",
    "test:gmail-simple": "node comprehensive-gmail-search-cli.js --maxUsersPerDomain 2 --maxResultsPerUser 1",
    "publish:task": "deno run -A taskcode/publish.ts --specific comprehensive-gmail-search"
  },
  "dependencies": {
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
